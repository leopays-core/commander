{
  "name": "mlrd-commander",
  "productName": "mlrd-commander",
  "version": "0.1.0",
  "private": true,
  "description": "MLRD commander",
  "keywords": [
    "LeoPays"
  ],
  "author": {
    "name": "LeoPays",
    "email": "dev@LeoPays.com",
    "url": "https://LeoPays.com/"
  },
  "homepage": "https://github.com/mlrd-chain/commander#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlrd-chain/commander.git"
  },
  "bugs": {
    "url": "https://github.com/mlrd-chain/commander/issues",
    "email": "dev@LeoPays.com"
  },
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "nodemon ./wait-react.js",
    "dev": "nodemon ./wait-react.js --debug"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "dockerode": "^3.0.2",
    "electron-log": "^4.0.3",
    "electron-settings": "^3.2.0",
    "electron-shortcut-normalizer": "^1.0.0",
    "glob": "^7.1.6",
    "highlight.js": "^9.17.1",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@octokit/rest": "^16.36.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "check-for-leaks": "^1.2.1",
    "devtron": "^1.4.0",
    "electron": "^7.1.9",
    "electron-packager": "^14.2.0",
    "electron-winstaller": "^4.0.0",
    "husky": "^4.0.10",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "signcode": "^1.0.0",
    "spectron": "^9.0.0",
    "standard": "^14.3.1"
  },
  "nodemonConfig": {
    "verbose": false,
    "delay": "2500",
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "exec2": "electron"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  },
  "build": {
    "appId": "money.milliard.mlrd",
    "productName": "mlrd-commander",
    "asar": true,
    "protocols": [
      {
        "name": "mlrd",
        "schemes": [
          "mlrd"
        ]
      }
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "createDesktopShortcut": true
    },
    "squirrelWindows": {
      "msi": true
    },
    "directories": {
      "output": "distribution"
    }
  }
}
